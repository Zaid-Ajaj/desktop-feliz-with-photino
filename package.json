{
    "private": true,
    "scripts": {
        "prestart": "dotnet tool restore && dotnet fable clean --yes",
        "start": "dotnet fable watch ./UserInterface --run webpack serve --mode=development",
        "prebuild": "dotnet tool restore && dotnet fable clean --yes",
        "build": "dotnet fable ./UserInterface && webpack --mode=production",
        "test:live": "dotnet fable watch ./UserInterface.Tests --run webpack serve --mode=development --config ./webpack.tests.js",
        "build:test": "dotnet fable ./UserInterface.Tests && webpack --config webpack.tests.js",
        "pretest": "dotnet tool restore",
        "test": "dotnet fable ./UserInterface.Tests --outDir dist/tests && mocha dist/tests -r esm"
    },
    "dependencies": {
        "bulma": "^0.9.1",
        "date-fns": "^2.16.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/runtime": "^7.12.5",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^6.4.1",
        "core-js": "^3.8.2",
        "css-loader": "^5.0.1",
        "dotenv-webpack": "^6.0.0",
        "esm": "^3.2.25",
        "file-loader": "^6.2.0",
        "graceful-fs": "^4.2.4",
        "html-webpack-plugin": "^4.5.1",
        "mini-css-extract-plugin": "^1.3.4",
        "mocha": "^8.2.1",
        "react-refresh": "^0.9.0",
        "sass": "^1.32.4",
        "sass-loader": "^10.1.1",
        "style-loader": "^2.0.0",
        "terser": "^4.8.0",
        "webpack": "^5.14.0",
        "webpack-cli": "^4.3.1",
        "webpack-dev-server": "^3.11.2"
    }
}
